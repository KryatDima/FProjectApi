@model FProjectMVC.Models.BrandListViewModel

@{
    ViewData["Title"] = "GetAllBrands";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Brands: </h4>

<div>
    @foreach (var brand in Model.Brands)
    {
        <div>
            <div><h6>Title:@Html.ActionLink(brand.Title, "GetBrandById", "Brand", new { id = brand.Id }, null)</h6></div>
        </div>
    }
<div>
    <button type="submit">@Html.ActionLink("Add New Brand", "Add", "Brand")</button>
</div>
</div>